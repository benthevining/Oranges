{
  "encode": {
    "emit_byteorder_mark": false,
    "input_encoding": "utf-8",
    "output_encoding": "utf-8"
  },
  "format": {
    "always_wrap": [],
    "autosort": true,
    "command_case": "unchanged",
    "dangle_align": "child",
    "dangle_parens": false,
    "disable": false,
    "enable_sort": true,
    "fractional_tab_policy": "use-space",
    "keyword_case": "unchanged",
    "layout_passes": {},
    "line_ending": "unix",
    "line_width": 100,
    "max_lines_hwrap": 2,
    "max_pargs_hwrap": 6,
    "max_prefix_chars": 10,
    "max_rows_cmdline": 2,
    "max_subgroups_hwrap": 4,
    "min_prefix_chars": 4,
    "require_valid_layout": false,
    "separate_ctrl_name_with_space": false,
    "separate_fn_name_with_space": true,
    "tab_size": 4,
    "use_tabchars": true
  },
  "lint": {
    "argument_var_pattern": ".*",
    "disabled_codes": [
      "C0111",
      "C0301",
      "C0307",
      "E1126",
      "R0915",
      "E1120"
    ],
    "function_pattern": ".*",
    "global_var_pattern": ".*",
    "internal_var_pattern": ".*",
    "keyword_pattern": ".*",
    "local_var_pattern": ".*",
    "macro_pattern": ".*",
    "max_arguments": 5,
    "max_branches": 25,
    "max_conditionals_custom_parser": 2,
    "max_localvars": 15,
    "max_returns": 6,
    "max_statement_spacing": 2,
    "max_statements": 50,
    "min_statement_spacing": 1,
    "private_var_pattern": ".*",
    "public_var_pattern": ".*"
  },
  "markup": {
    "bullet_char": "*",
    "canonicalize_hashrulers": false,
    "enable_markup": true,
    "enum_char": ".",
    "explicit_trailing_pattern": "#<",
    "fence_pattern": "^\\s*([`~]{3}[`~]*)(.*)$",
    "first_comment_is_literal": true,
    "hashruler_min_length": 10,
    "literal_comment_pattern": "^#\\[\\[",
    "ruler_pattern": "^\\s*[^\\w\\s]{3}.*[^\\w\\s]{3}$"
  },
  "misc": {
    "per_command": {}
  },
  "parse": {
    "additional_commands": {
      "autotools_add_build_target": {
        "kwargs": {
          "COMMENT": 1,
          "SOURCE_DIR": 1,
          "TARGET": 1
        }
      },
      "autotools_run_configure": {
        "kwargs": {
          "CXX_FLAGS": "+",
          "C_FLAGS": "+",
          "LD_FLAGS": "+",
          "SOURCE_DIR": 1
        },
        "pargs": {
          "flags": [
            "STATIC",
            "SHARED"
          ]
        }
      },
      "auval_add_plugin_test": {
        "kwargs": {
          "NAME": 1,
          "REPEATS": 1,
          "TARGET": 1
        }
      },
      "codesign_sign_plugin_targets": {
        "kwargs": {
          "TARGET": 1
        }
      },
      "codesign_sign_target": {
        "kwargs": {
          "TARGET": 1
        }
      },
      "include_external_xcode_project": {
        "kwargs": {
          "COMMENT": 1,
          "DIRECTORY": 1,
          "EXTRA_ARGS": 1,
          "SCHEME": 1,
          "TARGET": 1
        }
      },
      "lemons_make_path_absolute": {
        "kwargs": {
          "BASE_DIR": 1,
          "VAR": 1
        }
      },
      "lemons_subdir_list": {
        "kwargs": {
          "BASE_DIR": 1,
          "DIR": 1,
          "RESULT": 1
        },
        "pargs": {
          "flags": [
            "RECURSE",
            "FILES",
            "FULL_PATHS"
          ]
        }
      },
      "oranges_create_doxygen_target": {
        "kwargs": {
          "FILE_PATTERNS": "+",
          "IMAGE_PATHS": "+",
          "INPUT_PATHS": "+",
          "INSTALL_COMPONENT": 1,
          "LOGO": 1,
          "MAIN_PAGE_MD_FILE": 1,
          "OUTPUT_DIR": 1,
          "TARGET": 1
        },
        "pargs": {
          "flags": [
            "NO_VERSION_DISPLAY",
            "NO_INSTALL"
          ]
        }
      },
      "oranges_create_pkgconfig_file": {
        "kwargs": {
          "DESCRIPTION": 1,
          "INCLUDE_REL_PATH": 1,
          "INSTALL_COMPONENT": 1,
          "INSTALL_DEST": 1,
          "NAME": 1,
          "OUTPUT_DIR": 1,
          "REQUIRES": "+",
          "TARGET": 1,
          "URL": 1,
          "VERSION": 1
        },
        "pargs": {
          "flags": "NO_INSTALL"
        }
      },
      "oranges_download_file": {
        "kwargs": {
          "COPY_TO": 1,
          "EXPECTED_HASH": 1,
          "FILENAME": 1,
          "GITHUB_REPOSITORY": 1,
          "GIT_BRANCH": 1,
          "PACKAGE_NAME": 1,
          "PASSWORD": 1,
          "PATH_OUTPUT": 1,
          "REPO_REL_PATH": 1,
          "TIMEOUT": 1,
          "URL": 1,
          "USERNAME": 1
        },
        "pargs": {
          "flags": [
            "NO_CACHE",
            "QUIET",
            "NEVER_LOCAL"
          ]
        }
      },
      "oranges_fetch_repository": {
        "kwargs": {
          "BITBUCKET_REPOSITORY": 1,
          "CMAKE_OPTIONS": "+",
          "CMAKE_SUBDIR": 1,
          "GITHUB_REPOSITORY": 1,
          "GITLAB_REPOSITORY": 1,
          "GIT_OPTIONS": "+",
          "GIT_REPOSITORY": 1,
          "GIT_STRATEGY": 1,
          "GIT_TAG": 1,
          "NAME": 1
        },
        "pargs": {
          "flags": [
            "NEVER_LOCAL",
            "QUIET",
            "DOWNLOAD_ONLY",
            "EXCLUDE_FROM_ALL",
            "FULL",
            "NO_SUBMODULES",
            "NO_RECURSE_SUBMODULES"
          ]
        }
      },
      "oranges_generate_export_header": {
        "kwargs": {
          "BASE_NAME": 1,
          "HEADER": 1,
          "INSTALL_COMPONENT": 1,
          "REL_PATH": 1,
          "TARGET": 1
        }
      },
      "oranges_generate_platform_header": {
        "kwargs": {
          "BASE_NAME": 1,
          "HEADER": 1,
          "INSTALL_COMPONENT": 1,
          "LANGUAGE": 1,
          "REL_PATH": 1,
          "TARGET": 1
        },
        "pargs": {
          "flags": "INTERFACE"
        }
      },
      "oranges_list_transform": {
        "kwargs": {
          "CALLBACK": 1,
          "LIST": 1,
          "OUTPUT": 1
        }
      },
      "pluginval_add_all_plugin_tests": {
        "kwargs": {
          "BLOCKSIZES": "+",
          "LEVEL": 1,
          "LOG_DIR": 1,
          "REPEATS": 1,
          "SAMPLERATES": "+",
          "TARGET": 1
        },
        "pargs": {
          "flags": [
            "NO_GUI",
            "VERBOSE",
            "RANDOMIZE"
          ]
        }
      },
      "pluginval_add_plugin_test": {
        "kwargs": {
          "BLOCKSIZES": "+",
          "LEVEL": 1,
          "LOG_DIR": 1,
          "NAME": 1,
          "REPEATS": 1,
          "SAMPLERATES": "+",
          "TARGET": 1
        },
        "pargs": {
          "flags": [
            "NO_GUI",
            "VERBOSE",
            "RANDOMIZE"
          ]
        }
      },
      "wraptool_configure_aax_plugin_signing": {
        "kwargs": {
          "ACCOUNT": 1,
          "GUID": 1,
          "KEYFILE": 1,
          "KEYPASSWORD": 1,
          "SIGNID": 1,
          "TARGET": 1
        }
      }
    },
    "override_spec": {},
    "proptags": [],
    "vartags": []
  }
}
