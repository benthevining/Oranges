{
  "encode": {
    "emit_byteorder_mark": false,
    "input_encoding": "utf-8",
    "output_encoding": "utf-8"
  },
  "format": {
    "autosort": true,
    "command_case": "canonical",
    "dangle_align": "prefix-indent",
    "dangle_parens": false,
    "disable": false,
    "enable_sort": true,
    "fractional_tab_policy": "use-space",
    "keyword_case": "upper",
    "line_ending": "unix",
    "line_width": 100,
    "max_lines_hwrap": 2,
    "max_pargs_hwrap": 6,
    "max_prefix_chars": 10,
    "max_rows_cmdline": 2,
    "max_subgroups_hwrap": 4,
    "min_prefix_chars": 4,
    "require_valid_layout": false,
    "separate_ctrl_name_with_space": true,
    "separate_fn_name_with_space": true,
    "tab_size": 4,
    "use_tabchars": false
  },
  "lint": {
    "argument_var_pattern": "[a-z][a-z0-9_]+",
    "disabled_codes": [
      "C0111",
      "C0301",
      "C0307",
      "R0915",
      "E1120",
      "E1126"
    ],
    "function_pattern": ".*",
    "global_var_pattern": "^ORANGES_",
    "internal_var_pattern": "[a-zA-Z][0-9a-zA-Z_]+",
    "keyword_pattern": "[A-Z][0-9A-Z_]+",
    "local_var_pattern": "[a-zA-Z][a-zA-Z0-9_]+",
    "macro_pattern": ".*",
    "max_arguments": 5,
    "max_branches": 25,
    "max_conditionals_custom_parser": 2,
    "max_localvars": 15,
    "max_returns": 6,
    "max_statement_spacing": 2,
    "max_statements": 50,
    "min_statement_spacing": 1,
    "private_var_pattern": "[0-9a-z_]+",
    "public_var_pattern": "[a-zA-Z][0-9a-zA-Z_]+"
  },
  "markup": {
    "bullet_char": "*",
    "canonicalize_hashrulers": false,
    "enable_markup": true,
    "enum_char": ".",
    "explicit_trailing_pattern": "#<",
    "fence_pattern": "^\\s*([`~]{3}[`~]*)(.*)$",
    "first_comment_is_literal": true,
    "hashruler_min_length": 10,
    "literal_comment_pattern": "\\.rst:$",
    "ruler_pattern": "^\\s*[^\\w\\s]{3}.*[^\\w\\s]{3}$"
  },
  "parse": {
    "additional_commands": {
      "autotools_add_build_target": {
        "kwargs": {
          "COMMENT": 1,
          "SOURCE_DIR": 1,
          "TARGET": 1
        }
      },
      "autotools_run_configure": {
        "kwargs": {
          "CXX_FLAGS": "+",
          "C_FLAGS": "+",
          "LD_FLAGS": "+",
          "SOURCE_DIR": 1
        },
        "pargs": {
          "flags": [
            "STATIC",
            "SHARED"
          ]
        }
      },
      "auval_add_plugin_test": {
        "kwargs": {
          "NAME": 1,
          "REPEATS": 1,
          "TARGET": 1
        }
      },
      "codesign_sign_plugin_targets": {
        "kwargs": {
          "TARGET": 1
        }
      },
      "codesign_sign_target": {
        "kwargs": {
          "TARGET": 1
        }
      },
      "faust_add_generation_command": {
        "kwargs": {
          "CLASS_NAME": 1,
          "INPUT_FILE": 1,
          "OUTPUT_FILE": 1
        }
      },
      "faust_add_library": {
        "kwargs": {
          "CLASS_NAMES": "+",
          "INPUT_FILES": "+",
          "INSTALL_COMPONENT": 1,
          "INSTALL_REL_PATH": 1
        },
        "pargs": {
          "nargs": 1
        }
      },
      "include_external_xcode_project": {
        "kwargs": {
          "COMMENT": 1,
          "DIRECTORY": 1,
          "EXTRA_ARGS": 1,
          "SCHEME": 1,
          "TARGET": 1
        }
      },
      "oranges_add_build_type_macros": {
        "kwargs": {
          "BASE_NAME": 1,
          "SCOPE": 1
        },
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_add_graphviz_target": {
        "kwargs": {
          "BINARY_DIR": 1,
          "COPY_GRAPH_TO": 1,
          "EXTRA_CMAKE_OPTIONS": "+",
          "GRAPHVIZ_CONFIG_FILE": 1,
          "OUTPUT_DIR": 1,
          "SOURCE_DIR": 1,
          "TARGET": 1
        },
        "pargs": {
          "flags": [
            "ALL"
          ]
        }
      },
      "oranges_add_protobuf_files": {
        "kwargs": {
          "INSTALL_COMPONENT": 1,
          "INSTALL_DIR": 1,
          "PROTO_FILES": "+",
          "PY_FILES_OUT": 1,
          "TARGET": 1
        },
        "pargs": {
          "flags": [
            "NO_PYTHON",
            "NO_DESCRIPTORS"
          ]
        }
      },
      "oranges_add_protobuf_library": {
        "kwargs": {
          "INSTALL_COMPONENT": 1,
          "INSTALL_DIR": 1,
          "PROTO_FILES": "+",
          "TYPE": 1
        },
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_add_source_files": {
        "kwargs": {
          "DIRECTORY_NAME": 1,
          "FILES": "+",
          "INSTALL_COMPONENT": 1,
          "INSTALL_DIR": 1,
          "PUBLIC_HEADERS": "+",
          "TARGET": 1
        }
      },
      "oranges_add_swig_targets": {
        "kwargs": {
          "AGGREGATE_TARGET": 1,
          "BINARY_DIR": 1,
          "INPUT_LANGUAGE": 1,
          "LANGUAGES": "+",
          "MODULE_NAME": 1,
          "OUTPUT_VAR": 1,
          "SOURCES": "+",
          "TYPE": 1
        },
        "pargs": {
          "flags": "NO_AGGREGATE_TARGET"
        }
      },
      "oranges_create_doxygen_target": {
        "kwargs": {
          "FILE_PATTERNS": "+",
          "IMAGE_PATHS": "+",
          "INPUT_PATHS": "+",
          "INSTALL_COMPONENT": 1,
          "LOGO": 1,
          "MAIN_PAGE_MD_FILE": 1,
          "OUTPUT_DIR": 1,
          "TARGET": 1
        },
        "pargs": {
          "flags": [
            "NO_VERSION_DISPLAY",
            "NO_INSTALL"
          ]
        }
      },
      "oranges_create_pkgconfig_file": {
        "kwargs": {
          "DESCRIPTION": 1,
          "INCLUDE_REL_PATH": 1,
          "INSTALL_COMPONENT": 1,
          "INSTALL_DEST": 1,
          "NAME": 1,
          "OUTPUT_DIR": 1,
          "REQUIRES": "+",
          "URL": 1,
          "VERSION": 1
        },
        "pargs": {
          "flags": "NO_INSTALL",
          "nargs": 1
        }
      },
      "oranges_enable_ccache": {
        "kwargs": {
          "OPTIONS": "+",
          "TARGET": 1
        }
      },
      "oranges_enable_clang_tidy": {
        "kwargs": {
          "CONFIG_FILE": 1,
          "EXTRA_ARGS": "+",
          "LANGS": "+",
          "TARGET": 1
        }
      },
      "oranges_enable_cppcheck": {
        "kwargs": {
          "DISABLE": "+",
          "ENABLE": "+",
          "EXTRA_ARGS": "+",
          "LANGS": "+",
          "TARGET": 1
        }
      },
      "oranges_enable_cpplint": {
        "kwargs": {
          "EXTRA_ARGS": "+",
          "IGNORE": "+",
          "LANGS": "+",
          "TARGET": 1,
          "VERBOSITY": 1
        }
      },
      "oranges_enable_ipo": {
        "kwargs": {
          "TARGET": 1
        },
        "pargs": {
          "flags": [
            "INCLUDE_DEBUG"
          ]
        }
      },
      "oranges_enable_iwyu": {
        "kwargs": {
          "EXTRA_ARGS": "+",
          "LANGS": "+",
          "TARGET": 1
        }
      },
      "oranges_enable_static_analysis": {
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_enable_universal_binary": {
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_fetch_repository": {
        "kwargs": {
          "BITBUCKET_REPOSITORY": 1,
          "CMAKE_OPTIONS": "+",
          "CMAKE_SUBDIR": 1,
          "GITHUB_REPOSITORY": 1,
          "GITLAB_REPOSITORY": 1,
          "GIT_OPTIONS": "+",
          "GIT_REPOSITORY": 1,
          "GIT_STRATEGY": 1,
          "GIT_TAG": 1,
          "NAME": 1
        },
        "pargs": {
          "flags": [
            "NEVER_LOCAL",
            "QUIET",
            "DOWNLOAD_ONLY",
            "EXCLUDE_FROM_ALL",
            "FULL",
            "NO_SUBMODULES",
            "NO_RECURSE_SUBMODULES"
          ]
        }
      },
      "oranges_generate_export_header": {
        "kwargs": {
          "BASE_NAME": 1,
          "HEADER": 1,
          "INSTALL_COMPONENT": 1,
          "REL_PATH": 1,
          "SCOPE": 1
        },
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_generate_platform_header": {
        "kwargs": {
          "BASE_NAME": 1,
          "HEADER": 1,
          "INSTALL_COMPONENT": 1,
          "LANGUAGE": 1,
          "REL_PATH": 1,
          "SCOPE": 1
        },
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_generate_standard_headers": {
        "kwargs": {
          "BASE_NAME": 1,
          "BUILD_TYPE_HEADER": 1,
          "EXPORT_HEADER": 1,
          "FEATURE_TEST_LANGUAGE": 1,
          "HEADER": 1,
          "INSTALL_COMPONENT": 1,
          "PLATFORM_HEADER": 1,
          "REL_PATH": 1,
          "SCOPE": 1
        },
        "pargs": {
          "flags": [
            "NO_AGGREGATE_HEADER",
            "NO_BUILD_TYPE_MACROS"
          ],
          "nargs": 1
        }
      },
      "pluginval_add_all_plugin_tests": {
        "kwargs": {
          "BLOCKSIZES": "+",
          "LEVEL": 1,
          "LOG_DIR": 1,
          "REPEATS": 1,
          "SAMPLERATES": "+",
          "TARGET": 1
        },
        "pargs": {
          "flags": [
            "NO_GUI",
            "VERBOSE",
            "RANDOMIZE"
          ]
        }
      },
      "pluginval_add_plugin_test": {
        "kwargs": {
          "BLOCKSIZES": "+",
          "LEVEL": 1,
          "LOG_DIR": 1,
          "NAME": 1,
          "REPEATS": 1,
          "SAMPLERATES": "+",
          "TARGET": 1
        },
        "pargs": {
          "flags": [
            "NO_GUI",
            "VERBOSE",
            "RANDOMIZE"
          ]
        }
      },
      "wraptool_configure_aax_plugin_signing": {
        "kwargs": {
          "ACCOUNT": 1,
          "GUID": 1,
          "KEYFILE": 1,
          "KEYPASSWORD": 1,
          "SIGNID": 1,
          "TARGET": 1
        }
      }
    }
  }
}
