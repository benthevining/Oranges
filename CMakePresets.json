{
  "buildPresets": [
    {
      "cleanFirst": true,
      "configurePreset": "default",
      "name": "default"
    },
    {
      "configurePreset": "default",
      "name": "deps_graph",
      "targets": "DependencyGraph"
    },
    {
      "configurePreset": "default",
      "name": "docs",
      "targets": [
        "OrangesDoxygen",
        "DependencyGraph"
      ]
    }
  ],
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 21,
    "patch": 0
  },
  "configurePresets": [
    {
      "binaryDir": "${sourceDir}/Builds",
      "name": "default",
      "warnings": {
        "deprecated": true,
        "dev": true,
        "unusedCli": false
      }
    }
  ],
  "testPresets": [
    {
      "configurePreset": "default",
      "execution": {
        "noTestsAction": "error"
      },
      "filter": {
        "include": {
          "name": "Oranges."
        }
      },
      "name": "ORANGES_ROOT",
      "output": {
        "outputLogFile": "${sourceDir}/Builds/test_log.log",
        "outputOnFailure": true,
        "shortProgress": true,
        "verbosity": "verbose"
      }
    }
  ],
  "version": 3
}
