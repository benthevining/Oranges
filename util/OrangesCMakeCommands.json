{
  "parse": {
    "additional_commands": {
      "find_package_detect_macos_arch": {
        "pargs": {
          "nargs": 2
        }
      },
      "include_external_xcode_project": {
        "kwargs": {
          "COMMENT": 1,
          "DIRECTORY": 1,
          "EXTRA_ARGS": 1,
          "SCHEME": 1,
          "TARGET": 1
        }
      },
      "oranges_add_build_type_macros": {
        "kwargs": {
          "BASE_NAME": 1,
          "SCOPE": 1
        },
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_add_graphviz_target": {
        "kwargs": {
          "BINARY_DIR": 1,
          "COPY_GRAPH_TO": 1,
          "EXTRA_CMAKE_OPTIONS": "+",
          "GRAPHVIZ_CONFIG_FILE": 1,
          "OUTPUT_DIR": 1,
          "SOURCE_DIR": 1,
          "TARGET": 1
        },
        "pargs": {
          "flags": [
            "ALL"
          ]
        }
      },
      "oranges_add_source_files": {
        "kwargs": {
          "DIRECTORY_NAME": 1,
          "FILES": "+",
          "INSTALL_COMPONENT": 1,
          "INSTALL_DIR": 1,
          "PUBLIC_HEADERS": "+",
          "TARGET": 1
        }
      },
      "oranges_assert_target_argument_is_target": {
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_check_for_unparsed_args": {
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_create_doxygen_target": {
        "kwargs": {
          "FILE_PATTERNS": "+",
          "IMAGE_PATHS": "+",
          "INPUT_PATHS": "+",
          "INSTALL_COMPONENT": 1,
          "LOGO": 1,
          "MAIN_PAGE_MD_FILE": 1,
          "OUTPUT_DIR": 1,
          "TARGET": 1
        },
        "pargs": {
          "flags": [
            "NO_VERSION_DISPLAY",
            "NO_INSTALL"
          ]
        }
      },
      "oranges_create_pkgconfig_file": {
        "kwargs": {
          "DESCRIPTION": 1,
          "INCLUDE_REL_PATH": 1,
          "INSTALL_COMPONENT": 1,
          "INSTALL_DEST": 1,
          "NAME": 1,
          "OUTPUT_DIR": 1,
          "REQUIRES": "+",
          "URL": 1,
          "VERSION": 1
        },
        "pargs": {
          "flags": "NO_INSTALL",
          "nargs": 1
        }
      },
      "oranges_enable_ccache": {
        "kwargs": {
          "OPTIONS": "+"
        },
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_enable_clang_tidy": {
        "kwargs": {
          "CONFIG_FILE": 1,
          "EXTRA_ARGS": "+",
          "LANGS": "+"
        },
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_enable_cppcheck": {
        "kwargs": {
          "DISABLE": "+",
          "ENABLE": "+",
          "EXTRA_ARGS": "+",
          "LANGS": "+"
        },
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_enable_cpplint": {
        "kwargs": {
          "EXTRA_ARGS": "+",
          "IGNORE": "+",
          "LANGS": "+",
          "VERBOSITY": 1
        },
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_enable_ipo": {
        "pargs": {
          "flags": [
            "INCLUDE_DEBUG"
          ],
          "nargs": 1
        }
      },
      "oranges_enable_iwyu": {
        "kwargs": {
          "EXTRA_ARGS": "+",
          "LANGS": "+"
        },
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_enable_static_analysis": {
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_enable_universal_binary": {
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_generate_export_header": {
        "kwargs": {
          "BASE_NAME": 1,
          "HEADER": 1,
          "INSTALL_COMPONENT": 1,
          "REL_PATH": 1,
          "SCOPE": 1
        },
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_generate_platform_header": {
        "kwargs": {
          "BASE_NAME": 1,
          "HEADER": 1,
          "INSTALL_COMPONENT": 1,
          "LANGUAGE": 1,
          "REL_PATH": 1,
          "SCOPE": 1
        },
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_generate_standard_headers": {
        "kwargs": {
          "BASE_NAME": 1,
          "BUILD_TYPE_HEADER": 1,
          "EXPORT_HEADER": 1,
          "FEATURE_TEST_LANGUAGE": 1,
          "HEADER": 1,
          "INSTALL_COMPONENT": 1,
          "PLATFORM_HEADER": 1,
          "REL_PATH": 1,
          "SCOPE": 1
        },
        "pargs": {
          "flags": [
            "NO_AGGREGATE_HEADER"
          ],
          "nargs": 1
        }
      },
      "oranges_get_debug_config_list": {
        "pargs": {
          "nargs": 1
        }
      },
      "oranges_make_config_generator_expressions": {
        "kwargs": {
          "DEBUG": 1,
          "RELEASE": 1
        }
      }
    }
  }
}
